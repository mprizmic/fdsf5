<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8" />
    <title>
      {% block title %}
        Mis Marcadores!
      {% endblock %}
    </title>

    {% block stylesheets %}
      <link rel="stylesheet" href="/css/bootstrap.min.css" />
      <link rel="stylesheet" href="/css/open-iconic-bootstrap.css" />
      <link rel="stylesheet" href="/css/offcanvas.css" />

      <!-- lo agregue yo para los iconos de bootstrap pero no anda -->
      <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.7.2/font/bootstrap-icons.css" />
    {% endblock %}
  
  </head>
  
  <body class="bg-light">
    {% block body %}
  
      {% block menu %}
        {% include 'index/_menu.html.twig' %}
      {% endblock %}
  
      <div role="main" class="container">
        {% block contenedor_principal %}

        {% endblock %}
      </div>

      <div>
        {% block footer %}
          <span> {{ "aca va el footer" }}
            <a class="separador" style="" href="" >Contacto</a>
            <a class="separador" style="" href="" >Acerca de ...</a>
          </span>
        {% endblock %}
      </div>
        
      {% block javascript %}
        <script src="/js/jquery.min.js"></script>
        <script src="/js/bootstrap.min.js"></script>
        <script src="/js/offcanvas.js"></script>
      {% endblock %}
  
    {% endblock %}
  </body>
</html>
